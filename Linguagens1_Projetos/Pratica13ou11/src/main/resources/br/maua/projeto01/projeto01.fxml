<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.maua.projeto01.Proj1Controller">
   <children>
      <HBox>
         <children>
            <TextField fx:id="textField1" />
            <TextField fx:id="textField2" />
         </children>
      </HBox>
      <Label fx:id="mainLabel" text="TextLabel" />
      <HBox alignment="CENTER">
         <children>
            <Button fx:id="buttonHello" mnemonicParsing="false" text="Hello" />
            <Button fx:id="buttonText1" mnemonicParsing="false" text="Text1" />
            <Button fx:id="buttonSoma" text="Soma" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
